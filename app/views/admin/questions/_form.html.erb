<%= form_with model: [:admin, @test, @question], local: true do |f| %>
  <div class="form-group">
    <%= render "shared/errors", resource: @question %>

    <p>
      <%= f.label :body %>
      <%= f.text_field :body, id: :question_body, class: 'form-control' %>
    </p>

      <%= f.submit %>
  </div>
<% end %> 
